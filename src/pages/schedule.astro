---
import Layout from "@layouts/Page.astro";
import { Image } from "astro:assets";
import schedule from "@assets/schedule-1.png";
---

<Layout>
  <div class="mx-auto md:px-20 md:py-10">
    <h1 class="font-bold text-xl md:text-4xl text-center">Our Schedule</h1>
    <Image
      class="h-full object-center"
      src={schedule}
      alt="Strike Force and Fitness"
    />
  </div>
</Layout>
